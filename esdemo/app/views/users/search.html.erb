<div class="search-result-page">
  <table>
    <tr>
      <th class="name">Name</th>
      <th class="intro">Intro</th>
    </tr>

    <% @users.each do |u| %>
      <tr class="list">
        <td class="name"><%= u.name %></td>
        <td class="intro"><%= u.intro %></td>
      </tr>
    <% end %>
  </table>

  <%= link_to root_path, class: "back" do %>
    <i class="fa fa-arrow-left"> Back</i>
  <% end %>
</div>
